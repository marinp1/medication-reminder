{
  "name": "medication-reminder",
  "version": "1.0.4",
  "description": "Just a bot to tell me to take my meds",
  "main": "index.js",
  "author": "Patrik Marin",
  "license": "MIT",
  "scripts": {
    "build": "webpack --config webpack.prod.js",
    "start:dev": "NODE_ENV=development webpack --config webpack.dev.js",
    "run:dev": "NODE_ENV=development nodemon dist/bundle.js",
    "lint": "tslint -c tslint.json 'src/**/*.{ts,tsx}'",
    "prepack": "yarn build && rm -rf releases && mkdir releases",
    "copy-package-json": "cp package.json ./dist",
    "pack-tar": "tar -cvzf $npm_package_name-$npm_package_version.tar.gz ./dist && mv $npm_package_name-$npm_package_version.tar.gz releases",
    "pack-zip": "zip -vr $npm_package_name-$npm_package_version.zip ./dist/ && mv $npm_package_name-$npm_package_version.zip releases",
    "pack": "yarn run copy-package-json && yarn run pack-tar && yarn run pack-zip",
    "postpack": "npm version patch",
    "pretest": "npm run lint",
    "test": "jest"
  },
  "devDependencies": {
    "@types/dotenv": "^6.1.0",
    "@types/express": "^4.16.1",
    "@types/jest": "^24.0.12",
    "@types/nedb": "^1.8.7",
    "@types/node-fetch": "^2.1.6",
    "@types/node-telegram-bot-api": "^0.31.0",
    "@types/supertest": "^2.0.7",
    "@types/uuid": "^3.4.4",
    "@types/webpack": "^4.4.25",
    "concurrently": "^4.1.0",
    "file-loader": "^3.0.1",
    "fork-ts-checker-webpack-plugin": "^1.0.0",
    "jest": "^24.8.0",
    "nodemon": "^1.19.0",
    "prettier": "^1.16.4",
    "supertest": "^4.0.2",
    "ts-jest": "^24.0.2",
    "ts-loader": "^5.4.5",
    "tslint": "^5.14.0",
    "tslint-clean-code": "^0.2.9",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.3.3333",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.3.2",
    "webpack-node-externals": "^1.7.2",
    "webpack-shell-plugin": "^0.5.0"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "camunda-worker-node": "^7.1.0",
    "dayjs": "^1.8.9",
    "dotenv": "^7.0.0",
    "express": "^4.16.4",
    "nedb": "^1.8.0",
    "node-fetch": "^2.3.0",
    "node-telegram-bot-api": "^0.30.0",
    "uuid": "^3.3.2",
    "webpack-merge": "^4.2.1",
    "winston": "^3.2.1"
  }
}
