{
  "extends": [
    "tslint:recommended",
    "tslint-clean-code",
    "tslint-config-prettier"
  ],
  "rules": {
    "trailing-comma": true,
    "ordered-imports": false,
    "semicolon": [true, "always", "ignore-bound-class-methods"],
    "no-console": false,
    "object-literal-sort-keys": false,
    "max-classes-per-file": false,
    "quotemark": false,
    "newspaper-order": true
  }
}