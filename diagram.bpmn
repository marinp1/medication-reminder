    <bpmn:sequenceFlow id="SequenceFlow_0o5vleb" sourceRef="save-response" targetRef="end-success" />
    <bpmn:sequenceFlow id="SequenceFlow_12vji7d" sourceRef="catch-save-failure" targetRef="inform-user" />
