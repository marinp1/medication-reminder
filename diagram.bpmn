        <bpmn:targetRef>DataStoreReference_0tx3x3b</bpmn:targetRef>
      </bpmn:dataOutputAssociation>
    </bpmn:serviceTask>
    <bpmn:endEvent id="end-failure" name="Failure">
      <bpmn:incoming>SequenceFlow_0b86n4q</bpmn:incoming>
    </bpmn:endEvent>
